<h1>Create new User</h1>
<%= form_for :user, url: users_path do |f| %>
    <p>
      <%= f.label :UserName %><br>
      <%= f.text_field :username %>
    </p>

    <p>
      <%= f.label :Password %><br>
      <%= f.text_field :password %>
    </p>

    <p>
      <%= f.label :Roll %><br>
      <%= f.text_field :roll %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>
<%= link_to 'Back', users_path %>