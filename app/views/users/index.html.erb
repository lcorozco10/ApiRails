 <div class="table-responsive">
        <table class="table table-striped">
          <caption><h2>Listing User.</h2></caption>
          <tr>
            <th>ID</th>
            <th>User</th>
            <th>Password</th>
            <th>Roll</th>
            <th colspan="3">CRUD</th>

          </tr>

          <% @user.each do |user| %>
              <tr>
                <td><%= user.id %></td>
                <td><%= user.username %></td>
                <td><%= user.password %></td>
                <td><%= user.roll %></td>

                <td><%= link_to 'Show', user, class: "btn btn-success btn-xs", role:"button" %></td>
                <td><%= link_to 'Edit', edit_user_path(user), class:"btn btn-primary btn-xs", role:"button" %></td>
                <td><%= link_to 'Destroy', user,
                                method: :delete, data: { confirm: 'Are you sure?' },
                                class:"btn btn-danger btn-xs", role:"button" %></td>
              </tr>
          <% end %>
        </table>
</div>
<%= link_to 'CRUD', controller: 'users' %> /
<%= link_to 'New User', new_user_path %>